    "lyrics": [
        {
            "block_start": true,
            "chords": "Hmi               G",
            "text": "Kristínka by mala dnes"
        },
        {
            "chords": "   A                 G",
            "text": "už o dva roky dlhšie vlasy"
        },
        {
            "chords": "Hmi                     G",
            "text": "a plač mi skáče do slov vieš"
        },
        {
            "chords": "    A                 G",
            "text": "keď zas a znova premietam si"
        },
        {
            "chords": "Hmi              G",
            "text": "Ako vysí prichytená"
        },
        {
            "chords": "    A                G",
            "text": "v trhlinách vysokých brál"
        },
        {
            "chords": "      D               A",
            "text": "nikto nezachytí tvoje laná"
        },
        {
            "chords": "       G               A",
            "text": "keď sa rútiš s kolmých skál"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Bol som pri nej dole hneď"
        },
        {
            "chords": "",
            "text": "kým jej výkrik doznel v ozvenách"
        },
        {
            "chords": "",
            "text": "a za viečkami našiel tlieť"
        },
        {
            "chords": "",
            "text": "dve zľaknuté svetielka"
        },
        {
            "chords": "",
            "text": "Čo sa robí v mojich žílách"
        },
        {
            "chords": "",
            "text": "o to sa nik nestará"
        },
        {
            "chords": "",
            "text": "nikto nezachytí tvoje laná"
        },
        {
            "chords": "",
            "text": "keď sa rútiš s kolmých skál"
        },
        {
            "block_start": true,
            "chords": "D         A     Hmi",
            "text": "Spíš Kristínka, snáď len spíš"
        },
        {
            "chords": "     G         D",
            "text": "neverím poď a vstaň"
        },
        {
            "chords": "    Emi               A",
            "text": "ten pád bol možno len klam"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Spíš Kristínka, snáď len spíš"
        },
        {
            "chords": "",
            "text": "ty musíš hore ísť"
        },
        {
            "chords": "",
            "text": "k oblakom sme už na dotyk"
        },
        {
            "chords": "",
            "text": "Ty si jediná, z nás,"
        },
        {
            "chords": "",
            "text": "tak blízko pri oblakoch"
        },
        {
            "chords": "",
            "text": "Ty si jediná, z nás,"
        },
        {
            "chords": "",
            "text": "tak blízko pri oblakoch,"
        },
        {
            "chords": "",
            "text": "pri oblakoch"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Vydýchla mi v náručí"
        },
        {
            "chords": "",
            "text": "a od vtedy sa podvádzam,"
        },
        {
            "chords": "",
            "text": "že Kristínka snáď iba spí,"
        },
        {
            "chords": "",
            "text": "že ticho za ňu rozpráva"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Tak sa platí, za to že mladá"
        },
        {
            "chords": "",
            "text": "chcela vidieť z výšky brál"
        },
        {
            "chords": "",
            "text": "nikto nezachytí tvoje laná"
        },
        {
            "chords": "",
            "text": "keď sa rútiš s kolmých skál"
        },
        {
            "chords": "",
            "text": "Spíš Kristínka, snáď len spíš..."
        },
        {
            "chords": "",
            "text": "Ty si jediná, z nás…"
        },
        {
            "chords": "",
            "text": "Spíš Kristínka, snáď len spíš...  3x"
        }
