{
    "id": "neni-vsechno-parada-karel-zich",
    "title": "Není všechno paráda",
    "artist": [
        "Karel Zich"
    ],
    "band": [],
    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "chords": "C                        G",
            "text": "Není všechno paráda co hrajem,"
        },
        {
            "chords": "F           G          C",
            "text": "někdy stačí jenom zabrnkat,"
        },
        {
            "chords": "C                      G",
            "text": "kdo se pár not naučí a zná je,"
        },
        {
            "chords": "  F          G           C",
            "text": "k brnkání si najde třeba drát."
        },
        {
            "chords": "F",
            "text": "Bez parády začínal jsem tenkrát,"
        },
        {
            "chords": "G",
            "text": "s rozladěnou starou kytarou."
        },
        {
            "chords": "          C                        G",
            "text": "A trnem v oku byl mi školník Josef tenkrát,"
        },
        {
            "chords": "          F               G            C",
            "text": "když jsem přes plot tajně chodil za Klárou."
        },
        {
            "chords": "",
            "text": "Není všechno paráda co cítím,"
        },
        {
            "chords": "",
            "text": "někdy stačí škrtnout rozumem."
        },
        {
            "chords": "",
            "text": "Mám doma dvěstě dvacet silných voltů v síti,"
        },
        {
            "chords": "",
            "text": "brzy je mám tahat s sebou ven."
        },
        {
            "chords": "",
            "text": "Bez parády svítil jsem si sirkou,"
        },
        {
            "chords": "",
            "text": "na zvonkový štítek u dveří."
        },
        {
            "chords": "",
            "text": "Svou dívku přistihnul jsem s kamarádem Jirkou,"
        },
        {
            "chords": "",
            "text": "od těch dob mi Jirka nevěří."
        },
        {
            "chords": "",
            "text": "Není všechno paráda co dělám,"
        },
        {
            "chords": "",
            "text": "i když zrovna nezpůsobím zkrat."
        },
        {
            "chords": "",
            "text": "Žádná moje píseň nemusí být celá,"
        },
        {
            "chords": "",
            "text": ": stačí půlka aspoň chvíli mám co hrát :"
        }
    ]
}