{
    "id": "necekej-uz-dal-michal-tucny",
    "title": "Nečekej už dál",
    "artist": [
        "Michal Tučný"
    ],
    "band": [
        "Greenhorns"
    ],
    "originalKey": "E",
    "capo": 0,
    "lyrics": [
        {
            "chords": "E                H7",
            "text": "Nečekej už marně dál"
        },
        {
            "chords": "                 E",
            "text": "kdy se jezdec objeví."
        },
        {
            "chords": "       E7        A   E",
            "text": "Kde skončila cesta má"
        },
        {
            "chords": "       H7        E",
            "text": "jen vítr a písek ví."
        },
        {
            "chords": "   H7             E  H7",
            "text": "R: Vítr a písek to ví"
        },
        {
            "chords": "   H7              E  H7",
            "text": "   já tvý jméno nevolal"
        },
        {
            "chords": "   H7                     E",
            "text": "   v očích písek na rtech smích"
        },
        {
            "chords": "   F#  A       H7",
            "text": "   dál jsem se smál."
        },
        {
            "chords": "",
            "text": "Vítr pouště směr mi vzal"
        },
        {
            "chords": "",
            "text": "smál se i mýmu bloudění"
        },
        {
            "chords": "",
            "text": "ten vítr písně bláznů hrál"
        },
        {
            "chords": "",
            "text": "tvý slzy v písek promění."
        },
        {
            "chords": "",
            "text": "R."
        },
        {
            "chords": "",
            "text": "Nečekej už marně dál"
        },
        {
            "chords": "",
            "text": "nevrátím se, už to víš,"
        },
        {
            "chords": "",
            "text": "kůň můj tajnou stezku znal"
        },
        {
            "chords": "",
            "text": "do mraků se dívej výš."
        },
        {
            "chords": "",
            "text": "Mrak podivnej uvidíš,"
        },
        {
            "chords": "",
            "text": "co se jezdci podobá,"
        },
        {
            "chords": "",
            "text": "píseň jezdců písečných,"
        },
        {
            "chords": "",
            "text": "s větrem hvízdám si i já."
        }
    ]
}