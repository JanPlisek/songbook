    "lyrics": [
        {
            "chords": "   Dmi                    C",
            "text": "1. Výťah opäť nechodí tak zdolať 13 poschodí"
        },
        {
            "chords": "   Bb         C           Dmi",
            "text": "   zostáva mi znova po svojich"
        },
        {
            "chords": "",
            "text": "   na schodoch čosi šramotí a neón kde tu nesvieti"
        },
        {
            "chords": "",
            "text": "   ešte že sa po tme nebojím"
        },
        {
            "chords": "",
            "text": "2. Počuť hlasné stereo aj výstrahy pred neverou"
        },
        {
            "chords": "",
            "text": "   ktosi čosi vŕta v paneloch"
        },
        {
            "chords": "",
            "text": "   tatramatky ródeo sa mieša kde tu s operou"
        },
        {
            "chords": "",
            "text": "   všetko počuť cestou po schodoch"
        },
        {
            "chords": "          Dmi          C",
            "text": "R: Cestou po schodoch, po schodoch"
        },
        {
            "chords": "   Dmi      C",
            "text": "   poznávam poschodia"
        },
        {
            "chords": "          Dmi          C",
            "text": "   poznám po schidoch, po zvukoch"
        },
        {
            "chords": "   Bb        A    Dmi",
            "text": "   čo sme to za ľudia"
        },
        {
            "chords": "",
            "text": "3. Štekot smutnej kólie za premárnené prémie"
        },
        {
            "chords": "",
            "text": "   vyhráža sa manžel rozvodom"
        },
        {
            "chords": "",
            "text": "   Disko tenis árie kritika televízie"
        },
        {
            "chords": "",
            "text": "   oddnes chodím iba po schodoch"
        }
    ]
}