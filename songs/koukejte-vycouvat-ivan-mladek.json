{
    "id": "koukejte-vycouvat-ivan-mladek",
    "title": "Koukejte vycouvat",
    "artist": [
        "Ivan Mládek"
    ],
    "band": [],
    "originalKey": "D",
    "capo": 0,
    "lyrics": [
        {
            "chords": "D",
            "text": "Koukejte vycouvat, já před váma tu dávno byl,"
        },
        {
            "chords": "A7                              D",
            "text": "nechtějte abych se na vás rozzlobil."
        },
        {
            "chords": "",
            "text": "Čekal jsem, až ten pán místo pro mě vytvoří,"
        },
        {
            "chords": "A7                                     D",
            "text": "chtěl jsem tam zacouvat vše pro mě hovoří."
        },
        {
            "chords": "Hm                    F#7",
            "text": "Tváříte se že mě neslyšíte,"
        },
        {
            "chords": "Hm                   F#7",
            "text": "dobrá zkuste tedy odejít,"
        },
        {
            "chords": "Em         F#7        Hm   H7",
            "text": "vaše auto, až se navrátíte,"
        },
        {
            "chords": "   Em        F#7       Hm  (A7)",
            "text": "od desetníku rýhu bude mít."
        },
        {
            "chords": "",
            "text": "Když ten chlap odešel, odplaty čas nadešel,"
        },
        {
            "chords": "",
            "text": "já jemu s korunou auto obešel."
        },
        {
            "chords": "",
            "text": "Číhal však za rohem, čekal, jak se zachovám,"
        },
        {
            "chords": "",
            "text": "přešel mi po střeše, z auta ruinu mám."
        },
        {
            "chords": "",
            "text": "Náladu jsem přestal mít na fóry,"
        },
        {
            "chords": "",
            "text": "před očima se mi zatmělo,"
        },
        {
            "chords": "",
            "text": "vykop' jsem mu oba reflektory,"
        },
        {
            "chords": "",
            "text": "pak už bylo tělo na tělo."
        },
        {
            "chords": "",
            "text": "A tak primář kliniky s profesorem matiky"
        },
        {
            "chords": "",
            "text": "váleli se po zemi, žádný cavyky."
        },
        {
            "chords": "",
            "text": "Oba starší pánové, po té, když je zabásli,"
        },
        {
            "chords": "",
            "text": "po právu si přibližně stejně vyfásli."
        }
    ]
}