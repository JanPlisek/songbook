{
    "id": "lekorice-vaclav-neckar",
    "title": "Lékořice",
    "artist": [
        "Václav Neckář"
    ],
    "band": [],
    "originalKey": "F",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "F",
            "text": "Láska to je nemoc zlá, každý na ní svůj lék má,"
        },
        {
            "chords": "C7",
            "text": "poslechňete jak jsem na to od lesa šel já"
        },
        {
            "block_start": true,
            "chords": "F                           Bb    F",
            "text": "Rozhod jsem se léčit lásku lékořicí,"
        },
        {
            "chords": "C7                               F    C7",
            "text": "objednám si aspoň tisíc odnoží a-a-á"
        },
        {
            "chords": "",
            "text": "Pak si svařím soudek vína se skořicí"
        },
        {
            "chords": "",
            "text": "a počkám až se ten plevel rozmnoží."
        },
        {
            "block_start": true,
            "chords": " Bb                                 F             F7",
            "text": "Lékořicí - cesta ke mně ti zaroste jednou pro vždycky"
        },
        {
            "chords": " Bb                                F       Gmi7      C7",
            "text": "Lékořicí po víně se skořicí bývám cynický, ale fakticky."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "I své okno nechám zarůst lékořicí,"
        },
        {
            "chords": "",
            "text": "neuvidíš jak se trápím nelidsky."
        },
        {
            "chords": "",
            "text": "La-la ..."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Lékořicí - cesta ke mně ti zaroste jednou pro vždycky"
        },
        {
            "chords": "",
            "text": "lékořicí - jsi mé prokletí naprosté což je nelidský,"
        },
        {
            "chords": "",
            "text": "ale prakticky."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Kdybych věděl, že tě získám lékořicí"
        },
        {
            "chords": "",
            "text": "nemuselo by to končit tragicky."
        },
        {
            "chords": "",
            "text": "Dal bych rázně vale vínu se skořicí"
        },
        {
            "chords": "F              Dmi      H      C7",
            "text": "a za všechny svoje prachy:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "nakoupil bych hlínu a pěstoval inu"
        },
        {
            "chords": "",
            "text": "jenom tu květinu tu a žádnú inú"
        },
        {
            "chords": "",
            "text": "rostlo by napravo, rostlo by na levo"
        },
        {
            "chords": "",
            "text": "pro tebe má děvo jen to sladký dřevo"
        },
        {
            "chords": " Bb            F",
            "text": "lékořice samá lékořice, lelele"
        },
        {
            "chords": "",
            "text": "lékořice samá lékořice."
        }
    ]
}