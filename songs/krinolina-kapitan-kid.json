{
    "id": "krinolina-kapitan-kid",
    "title": "Krinolína",
    "artist": [
        "Kapitán Kid"
    ],
    "band": [],
    "originalKey": "G",
    "capo": 0,
    "lyrics": [
        {
            "chords": "            G        Emi    C    D7",
            "text": "1. Jsem v krinolíně lariatu oblečen,"
        },
        {
            "chords": "        G         Emi       C D7",
            "text": "   mou košilí je tichá savana"
        },
        {
            "chords": "        G               Emi        C     D7",
            "text": "   a v sedle sním svůj kostkovaný táhlý sen,"
        },
        {
            "chords": "       G         D7      G",
            "text": "   sen omšelého Dona Juana."
        },
        {
            "chords": "            H7         Emi",
            "text": "   Tisíce krásek voní šalvějí,"
        },
        {
            "chords": "        H7                G    D7",
            "text": "   mou lásku sosny stínem oplácí,"
        },
        {
            "chords": "        G          Emi            C     D7",
            "text": "   jen na koni se se mnou projet nechtějí,"
        },
        {
            "chords": "        G            D7         G A7 D",
            "text": "   dál ve větru sem-tam se kymácí."
        },
        {
            "chords": "            G                        D",
            "text": "R: Měsíc mě označkoval cejchovadlem lesů,"
        },
        {
            "chords": "               A7                  D",
            "text": "   sombrero z kaktusu si vyzývavě nesu,"
        },
        {
            "chords": "            G     Gmi   D   H7",
            "text": "   je moje láhev dávno dopita"
        },
        {
            "chords": "             Emi   A7  D D7",
            "text": "   a v dáli tepou kopyta."
        },
        {
            "chords": "",
            "text": "2. Že pistole už Oklamohou nezazní,"
        },
        {
            "chords": "",
            "text": "   to nemrzí mě, bude aspoň klid,"
        },
        {
            "chords": "",
            "text": "   a že se ráno jako jindy rozední,"
        },
        {
            "chords": "",
            "text": "   je lepší, nežli zlato objevit."
        },
        {
            "chords": "",
            "text": "   Hacienda v dáli není má"
        },
        {
            "chords": "",
            "text": "   a nepatří mi stáda kolem ní,"
        },
        {
            "chords": "",
            "text": "   a jestli mě tu vůbec něco dojímá,"
        },
        {
            "chords": "",
            "text": "   tak na dně kapsy tabák poslední."
        },
        {
            "chords": "",
            "text": "R:"
        },
        {
            "chords": "",
            "text": "3. I Llano Estacado zná můj zpěv,"
        },
        {
            "chords": "",
            "text": "   u Tennessee jsem řval do hřmění stád,"
        },
        {
            "chords": "",
            "text": "   jsem zkřivený a hořký jako babí hněv,"
        },
        {
            "chords": "",
            "text": "   ač mrtev, přesto žiju napořád."
        },
        {
            "chords": "",
            "text": "   V nábojích prach mám z dálných cest,"
        },
        {
            "chords": "",
            "text": "   já brodil řeky, projel údolí,"
        },
        {
            "chords": "",
            "text": "   žíznil jsem v poušti, viděl tisíc měst,"
        },
        {
            "chords": "",
            "text": "   mám koně, laso, hlad a pistoli."
        },
        {
            "chords": "",
            "text": "R:"
        }
    ]
}