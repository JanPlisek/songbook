        {
            "chords": "        A      D    A",
            "text": "1. Dítě školou povinné,"
        },
        {
            "chords": "              D     A",
            "text": "   dovede to, co my ne."
        },
        {
            "chords": "                      E7",
            "text": "   My jsme sice světa znalí,"
        },
        {
            "chords": "                        A",
            "text": "   my jsme lidé s rozhledem."
        },
        {
            "chords": "                    D",
            "text": "   Ale hrát si jako malí,"
        },
        {
            "chords": "          E7       A",
            "text": "   to bohužel nesvedem."
        },
        {
            "chords": "",
            "text": "2. Co se všechno nestane,"
        },
        {
            "chords": "",
            "text": "   když si dítě zamane."
        },
        {
            "chords": "",
            "text": "   Kuchyně se stane lesem"
        },
        {
            "chords": "",
            "text": "   a štokrdle medvědem."
        },
        {
            "chords": "",
            "text": "   Já i vy to těžce nesem,"
        },
        {
            "chords": "",
            "text": "   že to taky nesvedem."
        },
        {
            "chords": "   D7",
            "text": "R: Dítě věří tomu,"
        },
        {
            "chords": "      A               E7                   A",
            "text": "   že z domu do ulice vede třeba chodba tajná."
        },
        {
            "chords": "   D7            A",
            "text": "   O takové věci domovní důvěrnice"
        },
        {
            "chords": "   C#mi A7  C#mi  E",
            "text": "   nemá ani šajna."
        },
        {
            "chords": "",
            "text": "3. Dítě školou povinné,"
        },
        {
            "chords": "",
            "text": "   dovede to, co my ne."
        },
        {
            "chords": "",
            "text": "   Dejme tomu v elektrice"
        },
        {
            "chords": "",
            "text": "   průvodčí se rozplyne."
        },
        {
            "chords": "",
            "text": "   Dítě bloumá po Africe,"
        },
        {
            "chords": "",
            "text": "   loví lvice a my ne!"
