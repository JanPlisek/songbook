    "lyrics": [
        {
            "block_start": true,
            "chords": "   A7                     D7",
            "text": "1. Jednou mi fotr povídá, zůstali jsme už sami dva,"
        },
        {
            "chords": "       E7                        A7",
            "text": "   že si chce začít taky trochu žít,"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   nech si to projít palicí a nevracej se s vopicí,"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   snaž se mě hochu trochu pochopit."
        },
        {
            "block_start": true,
            "chords": "        E7                  A7",
            "text": "R: Já šel, šel dál, baby, kam mě Pánbůh zval,"
        },
        {
            "chords": "       E7                    D7",
            "text": "   já šel, šel dál, baby, a furt jen tancoval,"
        },
        {
            "chords": "   A7                      D7",
            "text": "   na každý divný hranici, na policejní stanici"
        },
        {
            "chords": "    E7                             A7",
            "text": "   hrál jsem jenom rock'n'roll for you."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Přiletěl se mnou černej čáp, zobákem dělal klapy klap"
        },
        {
            "chords": "",
            "text": "   a nad kolíbkou Elvis Presley stál,"
        },
        {
            "chords": "",
            "text": "   obrovskej bourák v ulici, po boku krásnou slepici"
        },
        {
            "chords": "",
            "text": "   a lidi šeptaj: přijel ňákej král."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R: Já šel, šel dál, baby, kam mě Pánbůh zval, ..."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Pořád tak ňák nemohu, chytit štěstí za nohu"
        },
        {
            "chords": "",
            "text": "   a nemůžu si najít klidnej kout,"
        },
        {
            "chords": "",
            "text": "   bláznivý ptáci začnou řvát a nový ráno šacovat"
        },
        {
            "chords": "",
            "text": "   a do mě pustí vždycky silnej proud."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R: Já šel, šel dál, baby, kam mě Pánbůh zval, ..."
        }
    ]
