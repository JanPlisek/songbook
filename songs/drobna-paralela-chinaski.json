    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "chords": "      C     G            D",
            "text": "1. Ta stará dobrá hra je okoukaná."
        },
        {
            "chords": "    C        G                 D",
            "text": "   Nediv se brácho, kdekdo ji zná."
        },
        {
            "chords": "    C          G                 D",
            "text": "   Přestaň se ptát, bylo nebylo líp."
        },
        {
            "chords": "    C        G                  D",
            "text": "   Včera je včera, bohužel bohudík."
        },
        {
            "chords": "    C             G     D           Em",
            "text": "R. Nic není jako dřív, nic není jak bejvávalo."
        },
        {
            "chords": "    C             G    D            Em",
            "text": "   Nic není jako dřív, to se nám to mívávalo."
        },
        {
            "chords": "    C             G     D                   Em",
            "text": "   Nic není jako dřív, ačkoliv máš všechno, co si vždycky chtěla"
        },
        {
            "chords": "    C             G     D                 Em",
            "text": "   Nic není jako dřív, ačkoliv drobná paralela by tu byla."
        },
        {
            "chords": "   C            G            D",
            "text": "2. Snad nevěříš na tajný znamení."
        },
        {
            "chords": "   C               G                D",
            "text": "   Všechno to harampádí - balábile - mámení."
        },
        {
            "chords": "   C              G              D",
            "text": "   Vážení platící, jak všeobecně ví se,"
        },
        {
            "chords": "   C               G              D",
            "text": "   včera i dneska, všude ta stará píseň."
        },
        {
            "chords": "",
            "text": "R. Ačkoliv nic není jako dřív ..."
        },
        {
            "chords": "    C               G",
            "text": "*: Promlouvám k vám ústy múzy,"
        },
        {
            "chords": "    D           D\/E",
            "text": "   vzývám tón a lehkou chůzi,"
        },
        {
            "chords": "    C             G",
            "text": "   vzývám zítřek nenadálý,"
        },
        {
            "chords": "    D          Em7",
            "text": "   odplouvám a mizím ..."
        },
        {
            "chords": "",
            "text": "R. Nic není jako dřív, nic není jak bejvávalo."
        },
        {
            "chords": "",
            "text": "   Nic není jako dřív, jó, to se nám to dlouze kouřívalo."
        },
        {
            "chords": "",
            "text": "   Bohužel bohudík je s námi, ta nenahmatatelná intimita těla."
        },
        {
            "chords": "",
            "text": "   Nic není jako dřív, jen fámy,"
        },
        {
            "chords": "",
            "text": "   bla - bla - bla - bla et cetera."
        },
        {
            "chords": "",
            "text": "   Nic není jako dřív, nic není jak bejvávalo,"
        },
        {
            "chords": "",
            "text": "   bohužel bohudík, co myslíš ségra, je to hodně nebo málo?"
        }
    ]
}