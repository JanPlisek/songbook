{
    "id": "nebude-to-ahke-richard-m-ller",
    "title": "Nebude to ľahké",
    "artist": [
        "Richard Müller"
    ],
    "band": [],
    "originalKey": "Am",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "   Ami             G",
            "text": "   Vraj zmenila si číslo"
        },
        {
            "chords": "   Ami        G                 F   G",
            "text": "   A do zámku nepasuje viac môj kľúč"
        },
        {
            "chords": "   Ami       G",
            "text": "   Hlavou mi blyslo"
        },
        {
            "chords": "   Ami       G              F",
            "text": "   Asi chceš aby som bol už fuč. Óu óuu"
        },
        {
            "block_start": true,
            "chords": "   C         G          F",
            "text": "R: Nebude to také ľahké drahá"
        },
        {
            "chords": "   C           G            F",
            "text": "   Mňa sa nezbavíš s tým sa lúč"
        },
        {
            "chords": "   C         G          F",
            "text": "   Nebude to také ľahké drahá"
        },
        {
            "chords": "   G          F           G",
            "text": "   Nemôžeš ma vymeniť ako rúž"
        },
        {
            "chords": "            Ami",
            "text": "   Som tvoj muž"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   Vraj koketuješ s iným"
        },
        {
            "chords": "",
            "text": "   Dolámem mu kosti, nos a sny"
        },
        {
            "chords": "",
            "text": "   Tak dopúšťaš sa viny"
        },
        {
            "chords": "",
            "text": "   Tak vrav, povedz čo máš s ním. Óu óuu"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   Vraj trháš moje fotky"
        },
        {
            "chords": "",
            "text": "   Majetok náš rozdelí až súd"
        },
        {
            "chords": "",
            "text": "   Ja exujem veľké vodky"
        },
        {
            "chords": "",
            "text": "   A márne sa snažím zachovať kľud. Óu óuu"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   Predsa sme si súdený"
        },
        {
            "chords": "",
            "text": "   Ja nemôžem žiť bez ženy"
        },
        {
            "chords": "",
            "text": "   Ja nemôžem žiť bez teba to mi ver, Oú"
        },
        {
            "chords": "",
            "text": "   A v kostole pred pánom"
        },
        {
            "chords": "",
            "text": "   Sľubovala si nám dvom"
        },
        {
            "chords": "",
            "text": "   Nekonečnú lásku, vernosť, nie, nie, nie, Óu óuu"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "   Vraj zmenila si číslo a do zámku nepasuje viac môj"
        }
    ]
}