        {
            "chords": "   D        F#   G",
            "text": "R: Hajný je lesa pán,"
        },
        {
            "chords": "   Emi     Emi  A7",
            "text": "   zvěří je milován"
        },
        {
            "chords": "   F#            Hmi",
            "text": "   Hajný je lesa král"
        },
        {
            "chords": "     G             A             D",
            "text": "   a každej pytlák se ho vždycky bál."
        },
        {
            "chords": "           B",
            "text": "Rec: Co se děje, les mi hoří ?"
        },
        {
            "chords": "",
            "text": "     Nebo řádí kuny, tchoři ?"
        },
        {
            "chords": "                  D",
            "text": "     Že by pytlák lesní pych ?"
        },
        {
            "chords": "",
            "text": "     Já tu meškám v pantoflích."
        },
        {
            "chords": "B",
            "text": "Klidně si zůstaňte v domácí obuvi"
        },
        {
            "chords": "G                 A",
            "text": "klukům to nevadí, dívky vás omluví,"
        },
        {
            "chords": "",
            "text": "děti: Máme výborný plán,"
        },
        {
            "chords": "",
            "text": "      tímto jste k němu zván,"
        },
        {
            "chords": "",
            "text": "      dejte nám stromů pár"
        },
        {
            "chords": "",
            "text": "      a dejte nám je třeba jako dar."
        },
        {
            "chords": "",
            "text": "Hajný - recitatív:"
        },
        {
            "chords": "",
            "text": "Sekerečku vem a zatni!"
        },
        {
            "chords": "",
            "text": "Copak to jde? Strom je státní!"
        },
        {
            "chords": "",
            "text": "Zadarmo se nezíská."
        },
        {
            "chords": "",
            "text": "Z toho koukaj želízka!"
        },
        {
            "chords": "",
            "text": "dítě - sólo:"
        },
        {
            "chords": "",
            "text": "Slibuju za kluky,"
        },
        {
            "chords": "",
            "text": "slibuju za holky:"
        },
        {
            "chords": "",
            "text": "za každý strom dáme"
        },
        {
            "chords": "",
            "text": "sto malých do školky."
        },
        {
            "chords": "",
            "text": "Hajný - sólo:"
        },
        {
            "chords": "",
            "text": "Tomu já říkám plán,"
        },
        {
            "chords": "",
            "text": "tímto je ujednán."
        },
        {
            "chords": "",
            "text": "Hajný je lesa pán,"
        },
        {
            "chords": "",
            "text": "je mládeží a zvěří milován."
        },
        {
            "chords": "",
            "text": "Hajný je lesa pán, je mládeží a zvěří milován."
        },
        {
            "chords": "aaaaa...",
            "text": "... hajný je lesa pán,"
        },
        {
            "chords": "",
            "text": "je mládeží a zvěří je milován."
        }
    ]
