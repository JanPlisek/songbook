        {
            "block_start": true,
            "chords": "   D            G     D",
            "text": "1. Dál za vozem spolu šlapem,"
        },
        {
            "chords": "         G           D",
            "text": "   někdo rád a někdo zmaten,"
        },
        {
            "chords": "          G           F#mi",
            "text": "   kdo se vrací, není sám,"
        },
        {
            "chords": "         Emi               G",
            "text": "   je to věc, když pro nás voní"
        },
        {
            "chords": "           D    G    D",
            "text": "   z hor divokej tymián."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Léto, zůstaň dlouho s námi,"
        },
        {
            "chords": "",
            "text": "   dlouho hřej a spal nám rány,"
        },
        {
            "chords": "",
            "text": "   až po okraj naplň džbán,"
        },
        {
            "chords": "",
            "text": "   je to věc, když pro nás voní"
        },
        {
            "chords": "",
            "text": "   z hor divokej tymián"
        },
        {
            "block_start": true,
            "chords": "   D7  G             D              G        F#mi",
            "text": "R: Podívej, jak málo stačí, když do vázy natrhám"
        },
        {
            "chords": "         Emi        G            D    G    D",
            "text": "   bílou nocí k milování z hor divokej tymián."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Dál za vozem trávou, prachem,"
        },
        {
            "chords": "",
            "text": "   někdy krokem, někdy trapem,"
        },
        {
            "chords": "",
            "text": "   kdo se vrací domů k nám,"
        },
        {
            "chords": "",
            "text": "   je to věc, když pro nás voní"
        },
        {
            "chords": "",
            "text": "   z hor divokej tymián."
        }
