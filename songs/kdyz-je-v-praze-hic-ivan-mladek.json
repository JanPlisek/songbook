{
    "id": "kdyz-je-v-praze-hic-ivan-mladek",
    "title": "Když je v Praze hic",
    "artist": [
        "Ivan Mládek"
    ],
    "band": [],
    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "chords": "   C",
            "text": "1. Když je v Praze abnormální hic,"
        },
        {
            "chords": "                            A7",
            "text": "   chodím k vodě až do Měchenic,"
        },
        {
            "chords": "   Dm         G7          C         A7",
            "text": "   měchenická plovárna má krásné okolí,"
        },
        {
            "chords": "   D7                        G7",
            "text": "   ještě krásnější než bazén v Praze čtyři - Podolí."
        },
        {
            "chords": "   C",
            "text": "   Má milá tam se mnou chodívá,"
        },
        {
            "chords": "                               A7",
            "text": "   chleba s máslem s sebou nosívá,"
        },
        {
            "chords": "   F            Fm           C              A7",
            "text": "   a když se jí šipka zdaří, úsměv na rtech má,"
        },
        {
            "chords": "   Dm              G7           C  (G7)",
            "text": "   chleba s máslem ukousnout mi dá."
        },
        {
            "chords": "",
            "text": "2. Když je v Praze abnormální mráz,"
        },
        {
            "chords": "",
            "text": "   do Měchenic táhne mě to zas,"
        },
        {
            "chords": "",
            "text": "   do prázdného bazénu tam s milou chodíme,"
        },
        {
            "chords": "",
            "text": "   na betonu někde na dně do očí si hledíme."
        },
        {
            "chords": "",
            "text": "   Neplavou tu žádné rybičky,"
        },
        {
            "chords": "",
            "text": "   dáváme si sladké hubičky,"
        },
        {
            "chords": "",
            "text": "   má milá je v dobré míře, úsměv na rtech má,"
        },
        {
            "chords": "",
            "text": "   chleba s máslem mi ukousnout dá."
        },
        {
            "chords": "",
            "text": "3. = 1."
        }
    ]
}