    "lyrics": [
        {
            "block_start": true,
            "chords": "          Ami     C      D     F",
            "text": "1. Snad znáš ten dům za New Orleans,"
        },
        {
            "chords": "        Ami   C          E",
            "text": "   ve štítu znak slunce má,"
        },
        {
            "chords": "          Ami       C        D        F",
            "text": "   je to dům, kde lká sto chlapců ubohejch"
        },
        {
            "chords": "        Ami        E       Ami C D F Ami E Ami E",
            "text": "   a v němž jsem zkejs' i já."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Mé mámě Bůh dal věnem"
        },
        {
            "chords": "",
            "text": "   jen prát a šít blue jeans,"
        },
        {
            "chords": "",
            "text": "   táta můj se flákal jen"
        },
        {
            "chords": "",
            "text": "   sám po New Orleans."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Bankrotář se zhroutil před hernou,"
        },
        {
            "chords": "",
            "text": "   jenom bídu svou měl a chlast,"
        },
        {
            "chords": "",
            "text": "   k putykám pak táh' tu pouť mizernou"
        },
        {
            "chords": "",
            "text": "   a znal jenom pít a krást."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "4. Být matkou, dám svým synům"
        },
        {
            "chords": "",
            "text": "   lepší dům, než má kdo z vás,"
        },
        {
            "chords": "",
            "text": "   ten dům, kde spím, má emblém sluneční,"
        },
        {
            "chords": "",
            "text": "   ale je v něm jen zima a chlad."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "5. Kdybych směl se hnout z těch kleští,"
        },
        {
            "chords": "",
            "text": "   pěstí vytrhnout tu mříž,"
        },
        {
            "chords": "",
            "text": "   já jak v snách bych šel do New Orleans"
        },
        {
            "chords": "",
            "text": "   a měl tam k slunci blíž."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "6.=1."
        }
    ]
