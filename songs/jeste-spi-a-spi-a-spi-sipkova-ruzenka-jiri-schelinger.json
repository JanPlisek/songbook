        {
            "block_start": true,
            "chords": "         Ami                    G",
            "text": "1. Ještě spí a spí a spí zámek šípkový,"
        },
        {
            "chords": "   Ami                      Emi",
            "text": "   žádný princ tam v lesích ptáky neloví,"
        },
        {
            "chords": "         Ami                    G",
            "text": "   ještě spí a spí a spí dívka zakletá,"
        },
        {
            "chords": "   Ami               Emi",
            "text": "   u lůžka jí planá růže rozkvétá."
        },
        {
            "block_start": true,
            "chords": "   C      D             Ami",
            "text": "R. To se schválně dětem říká,"
        },
        {
            "chords": "       F      G       C     E",
            "text": "   aby s důvěrou šli spát, klidně spát,"
        },
        {
            "chords": "         Ami       G       C           F",
            "text": "   že se dům probouzí a ta kráska procitá,"
        },
        {
            "chords": "         Dmi     Emi           Ami",
            "text": "   zatím spí tam dál, spí tam v růžích."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Kdo ji ústa k ústům dá, kdo ji zachrání,"
        },
        {
            "chords": "",
            "text": "   kdo si dívku pobledlou, vezme za paní,"
        },
        {
            "chords": "",
            "text": "   vyjdi zítra za ní a nevěř pohádkám,"
        },
        {
            "chords": "",
            "text": "   žádný princ už není, musíš tam jít sám."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R."
        },
        {
            "block_start": true,
            "chords": "   Ami          G           C          F",
            "text": "+  Musíš vyjít sám, nesmíš věřit pohádkám,"
        },
        {
            "chords": "       Dmi    Emi           Ami",
            "text": "   čeká dívka dál, spí tam v růžích,"
        },
        {
