{
    "id": "koukam-jak-cela-zeme-vstava-michal-tucny",
    "title": "Koukám, jak celá země vstává",
    "artist": [
        "Michal Tučný"
    ],
    "band": [
        "Fešáci"
    ],
    "originalKey": "G",
    "capo": 0,
    "lyrics": [
        {
            "chords": "  G             C             G",
            "text": "1.Ranním vlakem jezdím dlouhá léta"
        },
        {
            "chords": "                               D7",
            "text": "  v zimě, v létě, v každým počasí."
        },
        {
            "chords": "  G           C          G      Emi",
            "text": "  Postávám na pátým nástupišti,"
        },
        {
            "chords": "  A7                        D",
            "text": "  než si to ten courák přihasí."
        },
        {
            "chords": "      C",
            "text": "  Jen rozespalý průvodčí"
        },
        {
            "chords": "",
            "text": "  a pár chlápků se sny na očích,"
        },
        {
            "chords": "  G                       D7",
            "text": "  na aktovce začnou karty hrát."
        },
        {
            "chords": "  C",
            "text": "  Za oknem kraj ujíždí"
        },
        {
            "chords": "",
            "text": "  tím svým tempem hlemýždím"
        },
        {
            "chords": "        A7                  D7",
            "text": "  kolem polí a pasoucích se stád."
        },
        {
            "chords": "  G                     C",
            "text": "R.Koukám, jak celá země vstává"
        },
        {
            "chords": "    G                         D7",
            "text": "  a připadám si, jak ztracený syn,"
        },
        {
            "chords": "         C                       G      Emi",
            "text": "  já ten obraz dál v srdci přechovávám,"
        },
        {
            "chords": "        C           D7               G      Emi",
            "text": "  nežli noc na celý kraj položí svůj stín."
        },
        {
            "chords": "          C             D7       G",
            "text": "  To jsem já tvůj znovu nalezený syn."
        },
        {
            "chords": "",
            "text": "2.Jedem kolem nízkých spících domků,"
        },
        {
            "chords": "",
            "text": "  kolem starých aut a továrních hal."
        },
        {
            "chords": "",
            "text": "  Mávají nám květy jarních stromků,"
        },
        {
            "chords": "",
            "text": "  proč jsem si jich dřív už nevšímal."
        },
        {
            "chords": "",
            "text": "  Pak na každé další zastávce,"
        },
        {
            "chords": "",
            "text": "  ti co denně jezdí do práce,"
        },
        {
            "chords": "",
            "text": "  už navzájem představit se nemusí."
        },
        {
            "chords": "",
            "text": "  Jen hlavou kývnou na pozdrav,"
        },
        {
            "chords": "",
            "text": "  a tak jak káže dobrý mrav,"
        },
        {
            "chords": "",
            "text": "  palcem první cigaretu zadusí."
        },
        {
            "chords": "",
            "text": "R.Koukám, jak celá země vstává ..."
        },
        {
            "chords": "",
            "text": "3.Přidali nám cestou jeden vagón,"
        },
        {
            "chords": "",
            "text": "  zpoždění jsou vždycky zbytečná."
        },
        {
            "chords": "",
            "text": "  Strejda mi čte přes rameno z novin,"
        },
        {
            "chords": "",
            "text": "  za chvíli se blíží konečná."
        },
        {
            "chords": "",
            "text": "  Už je tlačenice, jak má být"
        },
        {
            "chords": "",
            "text": "  i průvodčí se musel probudit"
        },
        {
            "chords": "",
            "text": "  a chlápkové už nemají kde hrát."
        },
        {
            "chords": "",
            "text": "  Jen za oknem kraj ujíždí"
        },
        {
            "chords": "",
            "text": "  tím svým tempem hlemýždím"
        },
        {
            "chords": "",
            "text": "  kolem polí a pasoucích se stád."
        },
        {
            "chords": "",
            "text": "R.Koukám, jak celá země vstává ..."
        }
    ]
}