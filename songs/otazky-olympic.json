{
    "id": "otazky-olympic",
    "title": "Otázky",
    "artist": [],
    "band": [
        "Olympic"
    ],
    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "    C                    F",
            "text": "1. Kolik mám ještě dní, než přijde poslední,"
        },
        {
            "chords": "    G                               C",
            "text": "   jak dlouho budu  zpívat a hrát,"
        },
        {
            "chords": "                            F",
            "text": "   kolik je na zemi cest, kterou mám dát se vést,"
        },
        {
            "chords": "    G                        C",
            "text": "   nebo už myslet mám na návrat?"
        },
        {
            "block_start": true,
            "chords": "    C7                      F      D7                     G",
            "text": "R: Posečkej, lásko má, okamžik, vždyť svět je veliký otazník,"
        },
        {
            "chords": "    C                    F          G          C",
            "text": "   pořád se jenom ptáš a odpovědi, tý se nedočkáš."
        },
        {
            "block_start": true,
            "chords": "    E                        Ami",
            "text": "*: Já jen vím: v zimě strom nekvete,"
        },
        {
            "chords": "      Ami\/G     F         E        Ami",
            "text": "   v létě sníh nepadá, v noci je tma,"
        },
        {
            "chords": "           Ami\/G       F        C           G",
            "text": "   rád tě mám, jen neptej se, proč - nevím sám."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Kde je tvůj dětský smích a proč je láska hřích,"
        },
        {
            "chords": "",
            "text": "   kolik jen váží bolest člověčí,"
        },
        {
            "chords": "",
            "text": "   proč je zlato drahý kov a proč je tolik prázdných slov,"
        },
        {
            "chords": "",
            "text": "   proč chce každý být největší?"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "*: Já jen vím: řeky proud musi dál,"
        },
        {
            "chords": "",
            "text": "   v žilách krev pěnivá, kdo ji tam dal,"
        },
        {
            "chords": "",
            "text": "   rád tě mám, jen neptej se, proč - nevím sám."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Kolik je slunci let: milion nebo pět,"
        },
        {
            "chords": "",
            "text": "   myslíš, že zítra ráno vyjde zas,"
        },
        {
            "chords": "",
            "text": "   kolik je ulic, měst, proč umí kytky kvést,"
        },
        {
            "chords": "",
            "text": "   proč nikdo neslyší můj hlas?"
        }
    ]
}