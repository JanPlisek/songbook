    "lyrics": [
        {
            "block_start": true,
            "chords": "               Hmi  Emi           D",
            "text": "1. Když první tání cestu sněhu zkříží"
        },
        {
            "chords": "   G        Emi    F#       Hmi",
            "text": "   a nad ledem se voda objeví,"
        },
        {
            "chords": "                   Emi         D",
            "text": "   voňavá zem se sněhem tiše plíží,"
        },
        {
            "chords": "    G         Emi    F#              Hmi",
            "text": "   tak nějak líp si balím, proč, bůhví."
        },
        {
            "block_start": true,
            "chords": "                G                 D",
            "text": "R: Přišel čas slunce, zrození a tratí,"
        },
        {
            "chords": "                G                       D F#",
            "text": "   na kterejch potkáš kluky ze všech stran,"
        },
        {
            "chords": "               Hmi                     Emi",
            "text": "   : Hubenej Joe, Čára, Ušoun se ti vrátí,"
        },
        {
            "chords": "              G     F#            Hmi",
            "text": "      oživne kemp, jaro, vítej k nám. :"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Kdo ví, jak voní země, když se budí,"
        },
        {
            "chords": "",
            "text": "   pocit má vždy, jak zrodil by se sám,"
        },
        {
            "chords": "",
            "text": "   jaro je lék na řeči, co nás nudí,"
        },
        {
            "chords": "",
            "text": "   na lidi, co chtěj' zkazit život nám."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Zmrznout by měla, kéž by se tak stalo,"
        },
        {
            "chords": "",
            "text": "   srdce těch pánů, co je jim vše fuk,"
        },
        {
            "chords": "",
            "text": "   pak bych měl naději, že i příští jaro"
        },
        {
            "chords": "",
            "text": "   bude má země zdravá jako buk."
        },
        {
            "block_start": true,
            "chords": "             G     F#            Hmi",
            "text": "R: + oživne kemp, jaro, vítej k nám ..."
        }
    ]
