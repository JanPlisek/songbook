    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "   Ami G    Fmaj7     Emi7     Ami",
            "text": "1. Zpívám ptákům a zvlášť holubům,"
        },
        {
            "chords": "       G    Fmaj7  Emi7      Ami",
            "text": "   stával v údolí mém starý dům."
        },
        {
            "chords": "     C G  C        G         C",
            "text": "   Ptáků houf zalétal ke krovům,"
        },
        {
            "chords": "    Ami  G   Fmaj7   Emi7        Ami",
            "text": "   měl jsem rád holubích křídel šum."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Vlídná dívka jim házela hrách,"
        },
        {
            "chords": "",
            "text": "   mávání perutí víří prach."
        },
        {
            "chords": "",
            "text": "   Ptáci krouží a neznají strach,"
        },
        {
            "chords": "",
            "text": "   měl jsem rád starý dům, jeho práh."
        },
        {
            "block_start": true,
            "chords": "   Ami A7  Dmi     G            C         Ami",
            "text": "R: Hledám dům holubí, kdopak z vás cestu ví,"
        },
        {
            "chords": "           Dmi       G          C",
            "text": "   míval stáj roubenou, bílý štít."
        },
        {
            "chords": "        A7 Dmi     G       C          Ami",
            "text": "   Kde je dům holubí a ta dívka kde spí,"
        },
        {
            "chords": "             Dmi           Emi            Ami",
            "text": "   vždyť to ví, že jsem chtěl jen pro ni žít."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Sdílný déšť vypráví okapům,"
        },
        {
            "chords": "",
            "text": "   bláhový, kdo hledá tenhle dům."
        },
        {
            "chords": "",
            "text": "   Odrůstáš chlapeckým střevícům,"
        },
        {
            "chords": "",
            "text": "   neslyšíš holubích křídel šum."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "4. Nabízej úplatou cokoli,"
        },
        {
            "chords": "",
            "text": "   nepojíš cukrových homolí."
        },
        {
            "chords": "",
            "text": "   Můžeš mít třeba zrak sokolí,"
        },
        {
            "chords": "",
            "text": "   nespatříš ztracené údolí."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "  Zpívám ptákům a zvlášť holubům,"
        },
        {
            "chords": "",
            "text": "  stával v údolí mém starý dům ..."
        }
    ]
}