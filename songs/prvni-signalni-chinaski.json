    "lyrics": [
        {
            "chords": "   Emi   G           C          Emi",
            "text": "1. Až si zejtra ráno řeknu zase jednou provždy dost,"
        },
        {
            "chords": "   G            C          Emi",
            "text": "   právem se mi budeš tiše smát."
        },
        {
            "chords": "   G              C              Emi",
            "text": "   jak omluvit si svoji slabost, nenávist a zlost,"
        },
        {
            "chords": "   G                  C           Emi",
            "text": "   když za všechno si můžu vlastně sám."
        },
        {
            "chords": "   Ami                   C",
            "text": "R: Za spoustu dní možná  spoustu let,"
        },
        {
            "chords": "            G               D",
            "text": "   až se mi rozední budu ti vyprávět,"
        },
        {
            "chords": "   Ami                        C",
            "text": "   na první signální jak jsem vobletěl svět,"
        },
        {
            "chords": "             G        D",
            "text": "   jak tě to vomámí a nepustí zpět."
        },
        {
            "chords": "              F        Bb        Dmi",
            "text": "   Jaký si to uděláš - takový to máš,"
        },
        {
            "chords": "              F        Bb        Dmi",
            "text": "   jaký si to uděláš - takový to máš ."
        },
        {
            "chords": "   G                  C           Emi",
            "text": "2. Až se dneska večer budu tvářit zas jak Karel Gott,"
        },
        {
            "chords": "   G                C       Emi",
            "text": "   budu zpívat vampam - tydapam."
        },
        {
            "chords": "   G                   C         Emi",
            "text": "   Všechna sláva polní tráva ale peníz přijde vhod,"
        },
        {
            "chords": "   G              C           Emi",
            "text": "   jak jsem si to uďál tak to mám."
        },
