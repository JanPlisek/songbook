    "lyrics": [
        {
            "chords": "           C              F           C",
            "text": "Kolik je smutného, když mraky černé jdou "
        },
        {
            "chords": "              G7    F          C",
            "text": "lidem nad hlavou, smutnou dálavou, "
        },
        {
            "chords": "                         F             C",
            "text": "já slyšel příběh, který velkou pravdu měl, "
        },
        {
            "chords": "            G7   F          C",
            "text": "za čas odletěl, každý zapomněl. "
        },
        {
            "chords": "",
            "text": " "
        },
        {
            "chords": "    C          G7",
            "text": "R: Měl kapsu prázdnou Frenky dlouhán, "
        },
        {
            "chords": "               F             C",
            "text": "   po Státech toulal se jen sám, "
        },
        {
            "chords": "             F            C             G",
            "text": "   a že byl veselej, tak každej měl ho rád. "
        },
        {
            "chords": "               F                    C         Ami",
            "text": "   tam ruce k dílu mlčky přiloží a zase jede dál, "
        },
        {
            "chords": "      F                  G",
            "text": "   a každý, kdo s ním chvilku byl, "
        },
        {
            "chords": "         F     G7      C",
            "text": "   ten dlouho se pak smál. "
        },
        {
            "chords": "",
            "text": " "
        },
        {
            "chords": "",
            "text": "Tam, kde byl pláč, tam Frenky hezkou píseň měl, "
        },
        {
            "chords": "",
            "text": "slzy neměl rád, chtěl se jenom smát, "
        },
        {
            "chords": "",
            "text": "a když pak večer ranče tiše usínaj', "
        },
        {
            "chords": "",
            "text": "Frankův zpěv jde dál, nocí s písní dál. "
        },
        {
            "chords": "",
            "text": " "
        },
        {
            "chords": "",
            "text": "R: Měl kapsu prázdnou ... "
        },
        {
            "chords": "",
            "text": " "
        },
        {
            "chords": "",
            "text": "Tak jednou Frenkyho vám našli, přestal žít, "
        },
        {
            "chords": "",
            "text": "jeho srdce spí, tiše smutně spí, "
        },
        {
            "chords": "",
            "text": "bůh ví, jak, za co tenhle smíšek konec měl, "
        },
        {
            "chords": "",
            "text": "farář píseň pěl, umíráček zněl. "
        },
        {
            "chords": "",
            "text": " "
        },
        {
            "chords": "",
            "text": "R: Měl kapsu prázdnou ... "
        },
        {
