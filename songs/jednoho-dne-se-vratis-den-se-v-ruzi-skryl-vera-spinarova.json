    "lyrics": [
        {
            "chords": "A                                 D",
            "text": "Den se v růži skryl a z růže vůní hřál,"
        },
        {
            "chords": "        E                       A",
            "text": "tím jsi dlouho žil a kráse přísahal"
        },
        {
            "chords": "                                    D",
            "text": "Mám už v tváři smích, to léta můžou být,"
        },
        {
            "chords": "           E                             A",
            "text": "s prázdnem slůvek tvých chci znova písní znít"
        },
        {
            "chords": "   C#mi         F#mi A",
            "text": "To víš, že zůstanou,"
        },
        {
            "chords": "    D            A",
            "text": "jak loukám tváře kvést"
        },
        {
            "chords": "      F#mi               C#mi",
            "text": "Dál v knihách všech zázraků"
        },
        {
            "chords": "    D     E    A",
            "text": "mou písní zníš Adál"
        },
        {
            "chords": "   C#mi            F#mi A",
            "text": "To víš,že půjdem k nám,"
        },
        {
            "chords": "    D            A",
            "text": "jak cestám zbývá jít"
        },
        {
            "chords": "    F#mi          C#mi",
            "text": "Též dveřím co zívají"
        },
        {
            "chords": "    D     E    A",
            "text": "jak náruč toužení,"
        },
        {
            "chords": "   D         E     A",
            "text": "já dlaň tvou stále znám"
        },
        {
            "chords": "   C#mi         F#mi A",
            "text": "To víš sny zůstanou,"
        },
        {
            "chords": "  D              A",
            "text": "a možná může nás hřát"
        },
        {
            "chords": "    F#mi           C#mi",
            "text": "zas dál pár slůvek tvých,"
        },
        {
            "chords": "     D      E      A",
            "text": "když poznáš stůl a nůž"
        },
        {
            "chords": "                      F#mi              C#mi",
            "text": "když s příliš krásnou lží se loučit náleží"
        },
        {
            "chords": "      D                      E       A",
            "text": "vítej k nám, vítej k nám ten stůl tu máš"
        },
        {
            "chords": "   C#mi  F#mi A",
            "text": "Už náruč znáš,"
        },
        {
            "chords": "    D              A",
            "text": "jak chutná prázdná číš"
        },
        {
            "chords": "          F#mi             C#mi A",
            "text": "přijď zas k nám, ať sám už víš,"
        },
        {
            "chords": "    D       E     A",
            "text": "jak ve dvou dá se žít,"
        },
        {
            "chords": "   D       E      A",
            "text": "až příjdeš zpátky k nám."
        }
    ]
