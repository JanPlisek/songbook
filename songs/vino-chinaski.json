    "lyrics": [
        {
            "block_start": true,
            "chords": "C               G",
            "text": "Zase je pátek a mám toho dost"
        },
        {
            "chords": "         Bb              F",
            "text": "tak beru kramle, za zády Černý most"
        },
        {
            "chords": "  C              G",
            "text": "A pokud mi to D1 dovolí,"
        },
        {
            "chords": "         Bb                         C",
            "text": "než minu Jihlavu, hodím Prahu za hlavu."
        },
        {
            "block_start": true,
            "chords": "                      G",
            "text": "Jedu jak Pražák hlava nehlava,"
        },
        {
            "chords": "    Bb           F                 C",
            "text": "mám jediný cíl a přede mnou je Pálava"
        },
        {
            "chords": "                     G",
            "text": "Už od Brna jsem jako na trní,"
        },
        {
            "chords": "   Bb               F",
            "text": "ta sladká píseň tak blízko zní."
        },
        {
            "block_start": true,
            "chords": "  C            G",
            "text": "\/:My máme rádi víno,"
        },
        {
            "chords": "Bb       F             C",
            "text": "milujeme krásné ženy a zpěv"
        },
        {
            "chords": "               G",
            "text": "a na věc jdeme přímo,"
        },
        {
            "chords": "Bb           F                C",
            "text": "v žilách nám proudí Moravská krev :\/"
        },
        {
            "block_start": true,
            "chords": "C          G             Bb",
            "text": "Jako papír hodím práci a všechny"
        },
        {
            "chords": "     F",
            "text": "šéfy za hlavu,"
        },
        {
            "chords": "C       G                Bb",
            "text": "pánové, omlouvám se, ale já mizím"
        },
        {
            "chords": "   F",
            "text": "na Moravu."
        },
        {
            "block_start": true,
            "chords": "C                  G",
            "text": "Najdu tam všechno, co mi doma schází,"
        },
        {
            "chords": "Bb          F             C                  G",
            "text": "dobrý lid a klid a řád, najdu tam všechno, co na světě mám rád,"
        },
        {
            "chords": "Bb             F",
            "text": "vodu, slunce a nekonečný vinohrad."
        },
        {
            "block_start": true,
            "chords": "C            G    Bb       F             C",
            "text": "My máme rádi víno, milujeme krásné ženy a zpěv"
        },
        {
            "chords": "               G    Bb           F               C",
            "text": "a na věc jdeme přímo, v žilách nám proudí Moravská krev"
        },
        {
            "block_start": true,
            "chords": "              G    Bb          F            C",
            "text": "My tady totiž máme, všechno, co na světě máš rád"
        },
        {
            "chords": "              G    Bb       F                 Emi",
            "text": "a ochutnat ti dáme, slunce a nekonečný vinohrad."
        },
        {
            "block_start": true,
            "chords": "                  F",
