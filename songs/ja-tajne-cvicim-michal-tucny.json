{
    "id": "ja-tajne-cvicim-michal-tucny",
    "title": "Já tajně cvičím",
    "artist": [
        "Michal Tučný"
    ],
    "band": [
        "Fešáci"
    ],
    "originalKey": "G",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "      G",
            "text": "1. Chtěl bych začít cvičit gymnastiku,"
        },
        {
            "chords": "                               D",
            "text": "   chtěl bych dělat balet sólový,"
        },
        {
            "chords": "    D7",
            "text": "   tělocvičny denně leštit kliku"
        },
        {
            "chords": "                            G",
            "text": "   nebo vyhrát turnaj šachový."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Bůček, ovar, sádlo, nákyp, párky,"
        },
        {
            "chords": "",
            "text": "   svíčková či husí paštika,"
        },
        {
            "chords": "",
            "text": "   čokoláda, dorty, buchty, škvarky,"
        },
        {
            "chords": "",
            "text": "   tohleto mně už nic neříká."
        },
        {
            "block_start": true,
            "chords": "    G(G#)",
            "text": "R: Já sám už tajně cvičím roky,"
        },
        {
            "chords": "                              D(D#)",
            "text": "   za chvíli to na mně bude znát,"
        },
        {
            "chords": "    D7(D#7)",
            "text": "   zatím ovšem mám jen štíhlé boky,"
        },
        {
            "chords": "                             G(G#)",
            "text": "   švédská bedna je můj kamarád."
        },
        {
            "block_start": true,
            "chords": "    G#",
            "text": "3. Řekli mi: když plaveš, stoupne voda,"
        },
        {
            "chords": "                       D#",
            "text": "   není ze mě ani akrobat,"
        },
        {
            "chords": "    D#7",
            "text": "   na nářadí vzniká velká škoda,"
        },
        {
            "chords": "                                      G#",
            "text": "   přes to všechno mám sport strašně rád."
        },
        {
            "block_start": true,
            "chords": "      D#7",
            "text": "R: + zatím sice mám jen štíhlé boky,"
        },
        {
            "chords": "                             G#",
            "text": "     švédská bedna je můj kamarád."
        }
    ]
}