        {
            "block_start": true,
            "chords": "    C        C\/H     Ami    Ami7\/G",
            "text": "1. Po noci krátké a po milování"
        },
        {
            "chords": "     Ami7\/F#",
            "text": "   zbylo mi málo, jen hlava v dlaních,"
        },
        {
            "chords": "       C     G7              C  G G7",
            "text": "   co bolí, to je to laciný víno,"
        },
        {
            "chords": "    C       C\/H  Ami     Ami7\/G",
            "text": "   po noci plné cigaret kouře,"
        },
        {
            "chords": "    Ami7\/F#",
            "text": "   byls' jako tajfun, já jako bouře,"
        },
        {
            "chords": "       C     G               C",
            "text": "   to bolí, to je to laciný víno."
        },
        {
            "block_start": true,
            "chords": "    E                  E7",
            "text": "   Teď jenom matně v hlavě mi svítá,"
