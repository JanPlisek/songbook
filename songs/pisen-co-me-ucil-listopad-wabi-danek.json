{
    "id": "pisen-co-me-ucil-listopad-wabi-danek",
    "title": "Píseň, co mě učil listopad",
    "artist": [
        "Wabi Daněk"
    ],
    "band": [],
    "originalKey": "G",
    "capo": 2,
    "lyrics": [
        {
            "block_start": true,
            "chords": "    G          C           G          C",
            "text": "1. Málo jím a málo spím a málokdy tě vídám,"
        },
        {
            "chords": "    G          Hmi          Ami D7",
            "text": "   málokdy si nechám něco zdát,"
        },
        {
            "chords": "    C           G G\/F# Emi         C",
            "text": "   doma nemám stání už od jarního tání,"
        },
        {
            "chords": "    F                       G",
            "text": "   cítím, že se blíží listopad."
        },
        {
            "block_start": true,
            "chords": "             F       C            G",
            "text": "R: Listopadový písně od léta už slýchám,"
        },
        {
            "chords": "             Ami C           G",
            "text": "   vítr ledový přinesl je k nám,"
        },
        {
            "chords": "               F           C          G",
            "text": "   tak mě nečekej, dneska nikam nepospíchám,"
        },
        {
            "chords": "             Ami C           G",
            "text": "   listopadový písni naslouchám."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Chvíli stát a poslouchat, jak vítr větve čistí,"
        },
        {
            "chords": "",
            "text": "   k zemi padá zlatý vodopád,"
        },
        {
            "chords": "",
            "text": "   pod nohama cinká to poztrácené listí,"
        },
        {
            "chords": "",
            "text": "   vím, že právě zpívá listopad."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Dál a dál tou záplavou, co pod nohou se blýská,"
        },
        {
            "chords": "",
            "text": "   co mě nutí do zpěvu se dát,"
        },
        {
            "chords": "",
            "text": "   tak si chvíli zpívám a potom radši pískám"
        },
        {
            "chords": "",
            "text": "   píseň, co mě učil listopad."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        }
    ]
}