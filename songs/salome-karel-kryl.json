        {
            "chords": "     G",
            "text": "   plamen i červánek, ďábel i beránek,"
        },
        {
            "chords": "    E",
            "text": "   cukr i sůl,"
        },
        {
            "chords": "   Ami              C",
            "text": "   u vůně hřebíčku, u rytmu střevíčků"
        },
        {
            "chords": "    G",
            "text": "   císař dnes mi slíbil, za tanec přislíbil"
        },
        {
            "chords": "     E",
            "text": "   království půl."
        },
        {
            "block_start": true,
            "chords": "    Ami E   Ami",
            "text": "R: Salome, noc už je na sklonku,"
        },
        {
            "chords": "    C",
            "text": "   Salome, podobnas' úponku,"
        },
        {
            "chords": "    G",
            "text": "   podobna kytaře pro svého vladaře,"
        },
        {
            "chords": "    E",
            "text": "   Salome, tančíš."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R: Salome, sťali už Křtitele,"
        },
        {
            "chords": "",
            "text": "   Salome, usměj se vesele,"
        },
        {
            "chords": "",
            "text": "   točíš se ve víru, ústa jak upíru"
        },
        {
            "chords": "                                  Ami C F E Ami C E",
            "text": "   krví ti planou, Salome, la la la ..."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Noci už ubývá, císař se usmívá,"
        },
        {
            "chords": "",
            "text": "   pokojně mohu žít, všeho lze použít"
        },
        {
            "chords": "",
            "text": "   pro dobrý stát."
        },
        {
            "chords": "",
            "text": "   Možná ho napadlo, prastaré říkadlo:"
        },
        {
            "chords": "",
            "text": "   dějiny když tvoří se, pro hlavy na míse"
        },
        {
            "chords": "",
            "text": "   nemá se štkát."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R: Salome, netanči, nechceš-li,"
        },
        {
            "chords": "",
            "text": "   Salome, hosté už odešli,"
        },
        {
            "chords": "",
            "text": "   jenom roj komárů dopíjí z pohárů"
        },
        {
            "chords": "",
            "text": "   krůpěje vína."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R: Salome, trochu jsi pobledla,"
        },
        {
            "chords": "",
            "text": "   Salome, v koutku jsi usedla,"
        },
        {
            "chords": "",
            "text": "   víčka máš šedivá, nikdo se nedívá,"
        },
        {
            "chords": "                       Ami   E Ami C G E Ami",
