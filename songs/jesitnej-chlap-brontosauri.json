        {
            "chords": " Gmi      Dmi    Gmi       A7",
            "text": "Zapomeň, lásko, na to, že já"
        },
        {
            "chords": "      D                D7",
            "text": "jsem jenom ješitnej chlap,"
        },
        {
            "chords": "    Gmi      Dmi        Gmi      A7",
            "text": "že věčnej strach, abys byla jen má"
        },
        {
            "chords": "      D              D7",
            "text": "dělá ze mě horšího snad."
        },
        {
            "chords": "        Gmi                           A7",
            "text": "R: Jsi milá tak, že jako mrak jsou chvíle, kdy jsem sám,"
        },
        {
            "chords": "        D7                           Gmi",
            "text": "   kdy necítím tvůj dech na ústech svých,"
        },
        {
            "chords": "                                      A7",
            "text": "   jsi hezká tak, že jako mrak jsou tváře těch, co znám,"
        },
        {
            "chords": "      D7                           Gmi",
            "text": "   a pokřik vran je každej jinej smích."
        },
        {
            "chords": "",
            "text": "Snad ďábel sám mi napovídal,"
        },
        {
            "chords": "",
            "text": "že pláčeš kvůli mně,"
        },
        {
            "chords": "",
            "text": "jsem trdlo, který zapomíná,"
        },
        {
            "chords": "",
            "text": "jak málo lidí jako ty je."
        },
        {
            "chords": "",
            "text": "R: Jsi milá ..."
        },
        {
            "chords": "",
            "text": "Každý den, i když mračíš se snad,"
        },
        {
            "chords": "",
            "text": "provázíš můj každičkej krok,"
        },
        {
            "chords": "",
            "text": "v myšlenkách, slovech pořád jen dál"
        },
        {
            "chords": "",
            "text": "ozývá se: lásko, vždyť já jsem cvok."
        },
        {
            "chords": "",
            "text": "R: Jsi milá ..."
        }
    ]
