    "lyrics": [
        {
            "block_start": true,
            "chords": "Ami                                  Dmi",
            "text": "Někdy si říkám, jestli ještě znáš mý jméno,"
        },
        {
            "chords": "      G                             C    C\/H",
            "text": "a jak často na mě myslíš, když jdeš spát."
        },
        {
            "chords": "          Ami                  Dmi",
            "text": "Na zítřek v televizi hlásí zataženo,"
        },
        {
            "chords": "       E                     Ami",
            "text": "a v mý hlavě už tak bude napořád."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "Život jde dál, lásko, musíme si zvykat,"
        },
        {
            "chords": "",
            "text": "a není dobrý na tý cestě zůstat sám,"
        },
        {
            "chords": "",
            "text": "pro jeden smutek nemá cenu stokrát vzlykat,"
        },
        {
            "chords": "",
            "text": "když má svět otevřenou náruč dokořán."
        },
        {
            "block_start": true,
            "chords": "C                    Ami",
            "text": "A co bylo dál, jenom vítr,"
        },
        {
            "chords": "F          G         C",
            "text": "po nebi se mraky honily."
        },
        {
            "chords": "C                    Ami",
            "text": "A co bylo dál, jenom vítr,"
        },
        {
            "chords": "F              G          Ami",
            "text": "stromy se až k zemi sklonily."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "V uších mi pořád zněly tvoje něžný věty,"
        },
        {
            "chords": "",
            "text": "a do vlasů ti sedal bílej sníh,"
        },
        {
            "chords": "",
            "text": "z oblohy černý jak díra mezi světy,"
        },
        {
            "chords": "",
            "text": "se na mě snášel a studil na dlaních."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "A co bylo dál, jenom vítr,"
        },
        {
            "chords": "",
            "text": "po nebi se mraky honily."
        },
        {
            "chords": "",
            "text": "A co bylo dál, jenom vítr,"
        },
        {
            "chords": "",
            "text": "a stromy se až k zemi sklonily."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "V tom baru nevěděli, že je venku krize,"
        },
        {
            "chords": "",
            "text": "z rádia zpíval Cohen \"Lásko, jsem tvůj muž\","
        },
        {
            "chords": "",
            "text": "tys vešla dovnitř jak zhmotnění mý vize,"
        },
        {
            "chords": "",
            "text": "jako když do srdce mi někdo vrazí nůž..."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "A co bylo dál, jenom vítr,"
