{
    "id": "hraz-brontosauri",
    "title": "Hráz",
    "artist": [],
    "band": [
        "Brontosauři"
    ],
    "originalKey": "",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "     G                 Gmaj7\/F#          Gmaj7",
            "text": "1. Stál tam na stráni dům, v něm židle a stůl,"
        },
        {
            "chords": "               G                   Ami",
            "text": "   pár kůží a krb, co dřevo z něj voní,"
        },
        {
            "chords": "      D7",
            "text": "   s jarem, když máj rozdá barvy svý,"
        },
        {
            "chords": "                                     G  D7",
            "text": "   tu sosna krásná nad chajdou se skloní,"
        },
        {
            "chords": "      G                     Gmaj7\/F#           Gmaj7",
            "text": "   a říčka, když stříbrným hávem se přikryje s ránem,"
        },
        {
            "chords": "                G               E",
            "text": "   svý ahoj jí dáš a pak je tu den,"
        },
        {
            "chords": "    C                           Emi",
            "text": "   zazní údolím kytara tvá, ta píseň ráno uvítá,"
        },
        {
            "chords": "           D7          G D7",
            "text": "   svět s ním, svět s ním."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Touláš se po lese, touláš a jenom tak bloumáš"
        },
        {
            "chords": "",
            "text": "   a koruny stromů tě uvítaj' rosou,"
        },
        {
            "chords": "",
            "text": "   víš, že času je dost, to znáš,"
        },
        {
            "chords": "",
            "text": "   a možná potkáš někde dívku bosou,"
        },
        {
            "chords": "",
            "text": "   po slůvkách, který se říkaj', po dnech něžných stisků"
        },
        {
            "chords": "",
            "text": "   vás uvítá chajda a pak je tu den,"
        },
        {
            "chords": "",
            "text": "   zazní údolím kytara tvá, ta píseň ráno uvítá,"
        },
        {
            "chords": "",
            "text": "   svět s ním, svět s ním."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Však náhle volání táhlé ti přeruší snění"
        },
        {
            "chords": "",
            "text": "   a oznámí všem: je poslední den,"
        },
        {
            "chords": "",
            "text": "   voda zaplaví údolí,"
        },
        {
            "chords": "",
            "text": "   sosnu, chajdu, pohled zabolí,"
        },
        {
            "chords": "",
            "text": "   ta hráz je potřebná všem, však zabíjí den,"
        },
        {
            "chords": "",
            "text": "   co nosil tě v náručí romantickém,"
        },
        {
            "chords": "",
            "text": "   zazní údolím bolest tvá, ta bolest ráno uvítá,"
        },
        {
            "chords": "",
            "text": "   svět s ním, svět s ním ..."
        }
    ]
}