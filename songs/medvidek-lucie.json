    "lyrics": [
        {
            "chords": "D",
            "text": "Dlouhá noc a mě se stýská moc"
        },
        {
            "chords": "",
            "text": "pro tebe malý dárek mám."
        },
        {
            "chords": "         Emi",
            "text": "Přes hory přes ploty"
        },
        {
            "chords": "         A      D",
            "text": "medvídek z Bogoty už letí."
        },
        {
            "chords": "",
            "text": "Medvídek plyšový na cestě"
        },
        {
            "chords": "",
            "text": "křížový - Bůh ho ochrání."
        },
        {
            "chords": "         Emi",
            "text": "Před smečkou římskejch psů"
        },
        {
            "chords": "       A",
            "text": " a jejich úřadů"
        },
        {
            "chords": "   G                      A",
            "text": "ti posílá lásku co v bříšku má."
        },
        {
            "chords": "      Bb                D",
            "text": "Ref.: Medvídek z Bogoty usnul a sní"
        },
        {
            "chords": "      Bb              D",
            "text": "      na kříži z Bogoty spí."
        },
        {
            "chords": "                Emi",
            "text": "      Za třicet stříbrných"
        },
        {
            "chords": "                A",
            "text": "      z medvídka padá sníh"
        },
        {
            "chords": "             G",
            "text": "      no a ti římští psi"
        },
        {
            "chords": "                A                 D",
            "text": "      ho sjíždějí na saních o Vánocích."
        },
        {
            "chords": "",
            "text": "Nesvatá hodina medvídka proklíná"
        },
        {
            "chords": "",
            "text": "že odhodil korunu bez trnů"
        },
        {
            "chords": "",
            "text": "medvídek ospalý pod křížem pokleká"
        },
        {
            "chords": "",
            "text": "chce pít."
        },
        {
            "chords": "",
            "text": "Kalichem sladkost medvídka"
        },
        {
            "chords": "",
            "text": "napustí do bříška,jak to má rád."
        },
        {
            "chords": "",
            "text": "Koruna plyšová, vánoční cukroví,"
        },
        {
            "chords": "",
            "text": "nikdo se nedoví o svatozáři trnový."
        },
        {
            "chords": "",
            "text": "Ref.:Medvídek z Bogoty....."
        },
        {
            "chords": "Hmi",
            "text": "Medvídku probuď se, prober se vstaň,"
        },
        {
            "chords": "",
            "text": "psi se už sbíhají, připrav si dlaň"
        },
        {
            "chords": "A",
            "text": "slunce zas vychází cítíš tu zář,"
        },
        {
            "chords": "",
            "text": "tlapky dáš v pěst, nebo nastavíš tvář."
        },
        {
            "chords": " Bb                   D",
            "text": "Římskejm psům a jejich úřadům"
        },
        {
            "chords": " Bb                     A",
            "text": "Maxipes Herodes vánoční slib dal dnes."
        },
        {
            "chords": "",
            "text": "Ref.:   Medvídek"
        }
    ]
