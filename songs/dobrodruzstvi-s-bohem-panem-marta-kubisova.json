{
    "id": "dobrodruzstvi-s-bohem-panem-marta-kubisova",
    "title": "Dobrodružství s bohem Panem",
    "artist": [
        "Marta Kubišová"
    ],
    "band": [
        "Golden Kids"
    ],
    "originalKey": "Am",
    "capo": 0,
    "lyrics": [
        {
            "chords": "       Ami               G",
            "text": "1. Je půlnoc nádherná, spí i lucerna,"
        },
        {
            "chords": "    F              E",
            "text": "   tys' mě opustil ospalou,"
        },
        {
            "chords": "          Ami           G",
            "text": "   tu v hloubi zahrady cítím úklady,"
        },
        {
            "chords": "      F        E          Ami",
            "text": "   s píšťalou někdo sem kráčí,"
        },
        {
            "chords": "     C           G",
            "text": "   hrá náramně krásně a na mě"
        },
        {
            "chords": "    F            E",
            "text": "   tíha podivná doléhá,"
        },
        {
            "chords": "     C            G",
            "text": "   hrá náramně, zná mě, nezná mě,"
        },
        {
            "chords": "    F      E           Ami",
            "text": "   něha a hudba až k pláči."
        },
        {
            "chords": "",
            "text": "2. Pak náhle pomalu skládá píšťalu,"
        },
        {
            "chords": "",
            "text": "   krok, a slušně se uklání,"
        },
        {
            "chords": "",
            "text": "   jsem rázem ztracená, co to znamená,"
        },
        {
            "chords": "",
            "text": "   odháním strach, a on praví:"
        },
        {
            "chords": "",
            "text": "   Pan jméno mé, mám už renomé,"
        },
        {
            "chords": "",
            "text": "   Pan se jmenuju a jsem bůh,"
        },
        {
            "chords": "",
            "text": "   Pan, bůh všech stád, vás má, slečno, rád,"
        },
        {
            "chords": "",
            "text": "   jen Pan je pro vás ten pravý."
        },
        {
            "chords": "",
            "text": "3. Ráno, raníčko, ach, má písničko,"
        },
        {
            "chords": "",
            "text": "   Pan mi zmizel i s píšťalou,"
        },
        {
            "chords": "",
            "text": "   od Pana, propána, o vše obrána,"
        },
        {
            "chords": "",
            "text": "   ospalou najde mě máti,"
        },
        {
            "chords": "",
            "text": "   hrál a ve tmě krásně podved' mě,"
        },
        {
            "chords": "",
            "text": "   kam jsem to dala oči, kam,"
        },
        {
            "chords": "",
            "text": "   Pan, pěkný bůh, já teď nazdařbůh"
        },
        {
            "chords": "",
            "text": "   počítám \"dal\" a \"má dáti\"."
        }
    ]
}