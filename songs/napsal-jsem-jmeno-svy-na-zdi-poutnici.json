{
    "id": "napsal-jsem-jmeno-svy-na-zdi-poutnici",
    "title": "Napsal jsem jméno svý na zdi",
    "artist": [],
    "band": [
        "Poutníci"
    ],
    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "    C            G        C",
            "text": "1. Napsal jsem jméno svý na zdi,"
        },
        {
            "chords": "    F                      C",
            "text": "   na všechny zdi, který znám,"
        },
        {
            "chords": "    G            F        C",
            "text": "   napsal jsem jméno svý na zdi"
        },
        {
            "chords": "    F        C        G",
            "text": "   ráno, než otevřu krám,"
        },
        {
            "chords": "     C        G            C",
            "text": "   před léty ve stejnou chvíli"
        },
        {
            "chords": "       F                 C",
            "text": "   v pravici držel jsem nůž"
        },
        {
            "chords": "       G          F          C",
            "text": "   a všichni ti, co po něm zbyli,"
        },
        {
            "chords": "                G       C",
            "text": "   s nadšením hráli mi tuš."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. V okolí týhletý války"
        },
        {
            "chords": "",
            "text": "   tenkrát už stávala zeď,"
        },
        {
            "chords": "",
            "text": "   a popsaný popravčí špalky"
        },
        {
            "chords": "",
            "text": "   stály tam, co stojí teď,"
        },
        {
            "chords": "",
            "text": "   slyším zas slavičí hlasy"
        },
        {
            "chords": "",
            "text": "   a na tvářích rozlitej hněv,"
        },
        {
            "chords": "",
            "text": "   jak barvičky tý jejich krásy,"
        },
        {
            "chords": "",
            "text": "   která nám tak pije krev."
        },
        {
            "block_start": true,
            "chords": "     F                  C        G",
            "text": "R: Přiklušou z dáli s tím večerním tichem,"
        },
        {
            "chords": "      F               C        G",
            "text": "   k zemi se svalí a dusí se smíchem,"
        },
        {
            "chords": "    F                   C        G",
            "text": "   líce si hladí, než spálí je plamen,"
        },
        {
            "chords": "      F                   C          G",
            "text": "   a setřesou mládí jak zlou tíhu z ramen."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Z půlnoci rána jak kyjem"
        },
        {
            "chords": "",
            "text": "   a najednou, kdo by to řek',"
        },
        {
            "chords": "",
            "text": "   jsme chytří a trochu moc pijem"
        },
        {
            "chords": "",
            "text": "   a bijem svý děti, ten vztek."
        },
        {
            "chords": "",
            "text": "   Napsal jsem jméno svý na zdi"
        },
        {
            "chords": "",
            "text": "   a čekám, až setře je čas,"
        },
        {
            "chords": "",
            "text": "   napsal jsem jméno svý na zdi,"
        },
        {
            "chords": "",
            "text": "  |: stejně jak každýho z vás. :|"
        }
    ]
}