    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "      C       Dmi         C    Dmi",
            "text": "1. V zahradě za cihlovou zídkou,"
        },
        {
            "chords": "    C           Dmi         C  Dmi",
            "text": "   popsanou v slavných výročích,"
        },
        {
            "chords": "    C      Dmi       E         Ami",
            "text": "   sedává na podzim na trávě před besídkou"
        },
        {
            "chords": "    F          G(-)       E",
            "text": "   děvčátko s páskou na očích."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Pohádku o mluvícím ptáku"
        },
        {
            "chords": "",
            "text": "   nechá si přečíst z notesu,"
        },
        {
            "chords": "",
            "text": "   pak pošle polibek po chmýří na bodláku"
        },
        {
            "chords": "",
            "text": "   na vymyšlenou adresu."
        },
        {
            "block_start": true,
            "chords": "                Ami             Dmi",
            "text": "R: Prosím vás, nechte ji, ach, nechte ji,"
        },
        {
            "chords": "       Ami       G7",
            "text": "   tu nevidomou dívku,"
        },
        {
            "chords": "     Dmi        G7            E",
            "text": "   prosím vás, nechte ji si hrát,"
        },
        {
            "chords": "      Dmi                Ami",
            "text": "   vždyť možná hraje si na slunce s nebesy,"
        },
        {
            "chords": "    F(G)              G(E)          E",
            "text": "   jež nikdy neuvidí, ač ji bude hřát."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Pohádku o mluvícím ptáku"
        },
        {
            "chords": "",
            "text": "   a o třech zlatejch jabloních,"
        },
        {
            "chords": "",
            "text": "   a taky o lásce, již v černých květech máku"
        },
        {
            "chords": "",
            "text": "   přivezou jezdci na koních."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "4. Pohádku o kouzelném slůvku,"
        },
        {
            "chords": "",
            "text": "   jež vzbudí všechny zakleté,"
        },
        {
            "chords": "",
            "text": "   pohádku o duze, jež spává na ostrůvku,"
        },
        {
            "chords": "",
            "text": "   na kterém poklad najdete."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "chords": "",
            "text": "Rec.=1."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "5. Rukama dotýká se květů"
        },
        {
            "chords": "",
            "text": "   a neruší ji motýli,"
        },
        {
            "chords": "",
            "text": "   jen trochu hraje si s řetízkem amuletu,"
        },
        {
            "chords": "",
            "text": "   jen na chvíli."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        }
    ]
}