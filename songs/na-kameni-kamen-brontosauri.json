    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "         C",
            "text": "1. Jako suchej,starej strom,"
        },
        {
            "chords": "          Fmaj7",
            "text": "   jako všeničící hrom,"
        },
        {
            "chords": "                C",
            "text": "   jak v poli tráva"
        },
        {
            "chords": "                                   Fmaj7",
            "text": "   připadá mi ten náš svět, plnej řečí,"
        },
        {
            "chords": "          Ami      G      C",
            "text": "   a čím víc, tím líp se mám."
        },
        {
            "block_start": true,
            "chords": "         Fmaj7",
            "text": "R: Budem o něco se rvát,"
        },
        {
            "chords": "          Ami        G",
            "text": "   až tu nezůstane stát"
        },
        {
            "chords": "              C",
            "text": "   na kameni kámen,"
        },
        {
            "chords": "             Fmaj7",
            "text": "   a jestli není žádnej Bůh,"
        },
        {
            "chords": "            Ami            G",
            "text": "   tak nás vezme země - vzduch,"
        },
        {
            "chords": "               C",
            "text": "   no, a potom amen."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. A to všechno proto jen,"
        },
        {
            "chords": "",
            "text": "   že pár pánů chce mít den"
        },
        {
            "chords": "",
            "text": "   bohatší králů,"
        },
        {
            "chords": "",
            "text": "   přes všechna slova, co z nich jdou,"
        },
        {
            "chords": "",
            "text": "   hrabou pro kuličku svou, jen pro tu svou."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Možná jen se mi to zdá,"
        },
        {
            "chords": "",
            "text": "   a po těžký noci přijde,"
        },
        {
            "chords": "",
            "text": "   přijde hezký ráno,"
        },
        {
            "chords": "",
            "text": "   jaký bude, nevím sám,"
        },
        {
            "chords": "",
            "text": "   taky jsem si zvyk' na všechno kolem nás."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "R:"
        },
        {
            "chords": "    C         Fmaj7 Ami G C",
            "text": "   Laj la laj ..."
        }
    ]
}