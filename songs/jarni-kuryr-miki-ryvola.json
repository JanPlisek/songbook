{
    "id": "jarni-kuryr-miki-ryvola",
    "title": "Jarní kurýr",
    "artist": [
        "Miki Ryvola",
        "Wabi Ryvola"
    ],
    "band": [],
    "originalKey": "C",
    "capo": 0,
    "lyrics": [
        {
            "block_start": true,
            "chords": "       C        F           C",
            "text": "1. Dunění kopyt večer slýchávám,"
        },
        {
            "chords": "                Emi        E",
            "text": "   údolím jarní kurýr jede k nám,"
        },
        {
            "chords": "          Ami      G       F          E7",
            "text": "   v peřejích řeka zvoní a jarem vítr voní,"
        },
        {
            "chords": "        Ami       G      F  E7    Ami",
            "text": "   přijíždí jarní kurýr, dobře ho znám,"
        },
        {
            "chords": "             G",
            "text": "   Ví, celej kraj to ví,"
        },
        {
            "chords": "   F        G    F        E7   Ami",
            "text": "   veze nám jaro v brašně sedlový."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "2. Zase jdou krajem vánky voňavý,"
        },
        {
            "chords": "",
            "text": "   vobouvám svoje boty toulavý,"
        },
        {
            "chords": "",
            "text": "   dobře ví moje milá: i kdyby víla byla,"
        },
        {
            "chords": "",
            "text": "   tyhle toulavý boty nezastaví,"
        },
        {
            "chords": "",
            "text": "   mám boty toulavý,"
        },
        {
            "chords": "",
            "text": "   ty ani kouzlem nezastaví."
        },
        {
            "block_start": true,
            "chords": "",
            "text": "3. Musím jít, mraky táhnou nad hlavou,"
        },
        {
            "chords": "",
            "text": "   musím jít stopou bílou toulavou,"
        },
        {
            "chords": "",
            "text": "   neplakej, že se ztratím, já do roka se vrátím,"
        },
        {
            "chords": "",
            "text": "   prošlapám cestu domů jarní travou,"
        },
        {
            "chords": "",
            "text": "   víš, ty to dobře víš,"
        },
        {
            "chords": "",
            "text": "   ty moje boty nezastavíš."
        }
    ]
}